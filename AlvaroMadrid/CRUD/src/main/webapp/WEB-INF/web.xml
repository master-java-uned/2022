<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>AltaUsuario</servlet-name>
        <servlet-class>uned.master.java.crud.controlador.AltaUsuario</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AltaUsuario</servlet-name>
        <url-pattern>/AltaUsuario</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LoginUsuario</servlet-name>
        <servlet-class>uned.master.java.crud.controlador.LoginUsuario</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginUsuario</servlet-name>
        <url-pattern>/LoginUsuario</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ModificaUsuario</servlet-name>
        <servlet-class>uned.master.java.crud.controlador.ModificaUsuario</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ModificaUsuario</servlet-name>
        <url-pattern>/ModificaUsuario</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>EliminaUsuario</servlet-name>
        <servlet-class>uned.master.java.crud.controlador.EliminaUsuario</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EliminaUsuario</servlet-name>
        <url-pattern>/EliminaUsuario</url-pattern>
    </servlet-mapping>


    <welcome-file-list>
        <welcome-file>welcome.jsp</welcome-file>
        <welcome-file>logout.jsp</welcome-file>
    </welcome-file-list>
</web-app>