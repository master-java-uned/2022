package es.uned.master.java.models;

public class Casilla {
	
	private int PosicionAnterior;
	private int PosicionNueva;
	
	
	public Casilla(int posicionAnterior, int posicionNueva) {
		PosicionAnterior = posicionAnterior;
		PosicionNueva = posicionNueva;
	}
	
	public Casilla() {
	}

	
	public Casilla(int posicionAnterior) {
		PosicionAnterior = posicionAnterior;
	}

	public int getPosicionAnterior() {
		
		return PosicionAnterior;
	}
	public void setPosicionAnterior(int posicionAnterior) {
		if (posicionAnterior==0) {
			posicionAnterior=1;
		}
		PosicionAnterior = posicionAnterior;
	}
	
	public int getPosicionNueva() {
		return PosicionNueva;
	}
	public void setPosicionNueva(int posicionNueva) {
		PosicionNueva = posicionNueva;
	}
	
	
	
	

}